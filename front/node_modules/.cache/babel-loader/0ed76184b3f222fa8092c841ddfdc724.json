\n    <el-button @click=\"dialogVisible = false\">取 消</el-button>\n    <el-button type=\"primary\" @click=\"dialogVisible = false\">确 定</el-button>\n  </span>\n  <!-- 与newstudent绑定 -->\n  <div><span>学号</span><input v-model=\"newStudent.number\"/></div>\n  <div><span>姓名</span><input v-model=\"newStudent.name\"/></div>\n  <div><span>年龄</span><input v-model.number=\"newStudent.age\"/></div>\n  <div><span>语文</span><input v-model.number=\"newStudent.chi\"/></div>\n  <span>数学</span><input v-model.number=\"newStudent.math\"/>\n  <span>英语</span><input v-model.number=\"newStudent.eng\"/>\n</el-dialog>\n  </caption>\n  <thead>\n    <tr>\n      <th scope=\"col\">学号</th>\n      <th scope=\"col\">姓名</th>\n      <th scope=\"col\">年龄</th>\n      <th scope=\"col\">语文</th>\n      <th scope=\"col\">数学</th>\n      <th scope=\"col\">英语</th>\n      <th scope=\"col\">操作</th>\n    </tr>\n  </thead>\n  <tbody>\n    <Student v-for=\"stu in students\" :key=\"stu.id\" :student=\"stu\"></Student>\n  </tbody>\n</table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport Student from './components/Student.vue'\nexport default {\n  name: 'App',\n  components: {\n    Student\n  },\n  data() {\n    return{\n      dialogVisible: false,\n      students:[],\n      newStudent:{\n        number:\"\",\n        name:\"\",\n        age:\"\",\n        chi:\"\",\n        math:\"\",\n        eng:\"\"\n      }\n    }\n  },\n  methods:{\n    getStudent(){\n      axios({\n        url:\"http://localhost:8080/students\",\n        method:'GET',\n      }).then(res=>{\n        console.log(res.data);\n        this.students = res.data;\n      })\n    },\n    // 对话框\n    handleClose(done) {\n        this.$confirm('确认关闭？')\n          .then(_ => {\n            done();\n          })\n          .catch(_ => {});\n      }\n  },\n}\n</script>\n\n<style>\n\n</style>\n"]},"metadata":{},"sourceType":"module"}